{
  "manifest_version": 3,
  "name": "yrs",
  "version": "0",
  "description": "Yahoo Japan Realtime Searcher",
  "icons": {
    "128": "128.png"
  },
  "omnibox": {
    "keyword": "yrs"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "bg.js"
  },
  "content_scripts": [{
    "css": ["main.css"],
    "matches": ["https://search.yahoo.co.jp/realtime/*"],
    "run_at": "document_start"
  }]
}